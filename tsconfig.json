{
    "compilerOptions": {
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es2017",
        "paths": {
            "handlebars": ["./node_modules/handlebars/dist/handlebars.min.js"]
        } 
    },
    "exclude": [
        "./node_modules",
        "./.parcel-cache",
        "./dist",
        "./static"
    ],
    "include": [
        "./src"
    ]  
}